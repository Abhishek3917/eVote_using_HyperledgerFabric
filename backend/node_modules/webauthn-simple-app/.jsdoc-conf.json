{
    "tags": {
        "allowUnknownTags": false
    },
    "source": {
        "include": [ "classes", "lib" ],
        "includePattern": "\\.js$",
        "exclude": [ "node_modules", "test", "dist", "rollup.config.js", "GruntFile.js" ]
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "systemName": "WebAuthn Simple Application",
        "copyright": "Copyright 2018, Adam Powers",
        "template": "node_modules/docdash",
        "readme": "README.md",
        "encoding": "utf8",
        "destination": "docs/",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    },
    "docdash": {
        "static": true,
        "sort": true
    }
}